; COMPILE PTH100
;
 .IFINS FOR FOR PTH100=PTH100'$FOR1'
 .IFINS F4P F4P PTH100=PTH100'$F4P1'
;
; BUILD PTH100
;
   .OPEN  PTH100.CMD
  .DATA PTH100/CP,PTH100=PTH100,DGCOM/LB,LB:'$LUIC'LIB100/LB
   .DATA /
   .DATA UNITS=10
   .DATA COMMON=APDEV:RW:7
   .DATA //
   .CLOSE
 TKB @PTH100
 PIP PTH100.TSK/PU
; DELETE INTERMEDIATE FILES
 PIP PTH100.OBJ;*,PTH100.CMD;*/DE
 .EXIT
